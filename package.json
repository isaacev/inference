{
  "description": "is it possible to derrive a data model from just a template?",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isaacev/inference"
  },
  "scripts": {
    "build": "parcel build --out-file index.html --public-url ./ src/index.html",
    "check": "tsc --noEmit",
    "clean": "rimraf .cache dist",
    "cover": "npm test -- --coverage",
    "deploy": "npm run clean && npm run build && npm run slides && gh-pages -d dist",
    "fmt": "prettier --write src/*.ts src/*.js src/**/*.{ts,tsx}",
    "slides": "cp docs/slides.html dist",
    "start": "parcel --no-hmr --no-cache ./src/index.html",
    "test": "jest"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-pro": "^5.8.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/pro-regular-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/classnames": "^2.2.7",
    "@types/codemirror": "0.0.63",
    "@types/jest": "^24.0.11",
    "@types/node": "^10.14.4",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "classnames": "^2.2.6",
    "gh-pages": "^2.0.1",
    "jest": "^24.7.1",
    "node-sass": "^4.11.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "codemirror": "^5.45.0",
    "localforage": "^1.7.3",
    "react": "^16.8.6",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.8.6"
  }
}
